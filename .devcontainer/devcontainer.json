{
    "customizations": {
        "vscode": {
            "extensions": [
                "Prisma.prisma",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "postStartCommand": "cd api && npm install && npx prisma db push && npx prisma db seed && npm run start"
}